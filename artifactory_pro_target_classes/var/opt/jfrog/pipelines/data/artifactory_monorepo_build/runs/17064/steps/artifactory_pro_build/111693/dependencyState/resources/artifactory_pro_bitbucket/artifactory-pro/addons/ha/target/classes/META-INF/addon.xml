       http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean class="org.artifactory.addon.ha.HaAddonImpl" primary="true"/>
    <!--bean class="org.artifactory.addon.ha.propagate.HaPropagationServiceImpl"/-->
