<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean class="org.artifactory.addon.aol.webapp.AolCoreAddons" primary="true"/>

    <!-- Adding CAS authentication filter -->
    <bean class="org.artifactory.addon.aol.webapp.cas.CasAuthenticationFilter"/>

    <bean class="org.artifactory.addon.aol.webapp.cas.CasConfiguration"/>

    <bean class="org.artifactory.addon.aol.signed.url.SignedUrlAddonImpl"/>

    <!-- central config interceptor -->
    <bean class="org.artifactory.addon.aol.ConfigurationChangesValidationInterceptor"/>

</beans>